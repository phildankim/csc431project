#!/bin/bash

make clean;make

for dir in benchmarks/*; do
   f=${dir##*/}
   echo '-'$f
   java MiniCompiler $dir/${f}.mini -stack
   clang $dir/${f}.ll -m32
   ./a.out < $dir/input.longer > ${f}.txt
   echo 'diffing'
   diff ${f}.txt $dir/output.longer
   echo 'diffing complete'
done

echo 'benchmarks passed!!'
